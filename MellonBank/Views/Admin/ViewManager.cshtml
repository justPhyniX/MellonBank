@model MellonBank.Models.User


<h2>Manager Details</h2>
<table class="table">
    <tr>
        <th>First Name</th>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <th>Last Name</th>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>AFM</th>
        <td>@Model.AFM</td>
    </tr>
    <tr>
        <th>Username</th>
        <td>@Model.UserName</td>
    </tr>
</table>